/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And4Way {
   IN a, b, c, d;
   OUT out;
   
   PARTS:
	And(a = a, b = b, out = w1);
	And(a = c, b = d, out = w2);
	And(a = w1, b= w2, out = out);
}
